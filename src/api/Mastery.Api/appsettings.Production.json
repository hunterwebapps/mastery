{
  "KeyVault": {
    "VaultUri": "https://mastery-prod.vault.azure.net/"
  },
  "OpenAi": {
    "ApiKey": "",
    "MaxOutputTokens": 4096,
    "TimeoutSeconds": 60,
    "Enabled": true
  },
  "BackgroundWorker": {
    "Enabled": true,
    "IntervalHours": 3,
    "MaxUsersPerRun": 100,
    "TimeoutMinutesPerUser": 2
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    }
  }
}
